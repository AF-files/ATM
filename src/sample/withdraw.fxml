<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="withdrawPane" fx:controller="sample.withdrawCon" xmlns:fx="http://javafx.com/fxml" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
    <center>
        <GridPane alignment="CENTER" hgap="80" vgap="10">
            <Button fx:id="withdraw10" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.halignment="LEFT" onAction="#returnOrLeave">£10</Button>
            <Button fx:id="withdraw20" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.halignment="LEFT" onAction="#returnOrLeave">£20</Button>
            <Button fx:id="withdraw30" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.halignment="RIGHT" onAction="#returnOrLeave">£50</Button>
            <Button fx:id="withdraw40" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.halignment="RIGHT" onAction="#returnOrLeave">£100</Button>
        </GridPane>
    </center>
    <bottom>
        <HBox>
            <Button fx:id="goBackBut">Go Back</Button>
            <Button fx:id="exitBut">Exit</Button>
        </HBox>
    </bottom>
</BorderPane>

